<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.fakegooglenews.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_taiwan"
        android:name="com.example.fakegooglenews.ui.taiwan.TaiwanFragment"
        android:label="@string/title_taiwan"
        tools:layout="@layout/fragment_taiwan" />

    <fragment
        android:id="@+id/navigation_international"
        android:name="com.example.fakegooglenews.ui.international.InternationalFragment"
        android:label="@string/title_international"
        tools:layout="@layout/fragment_international" />
</navigation>